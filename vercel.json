{
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/spa",
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}